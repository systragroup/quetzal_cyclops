{
    "StartAt": "Cyclops Model",
    "States": {
      "Cyclops Model": {
        "Type": "Task",
        "Resource": "arn:aws:states:::lambda:invoke",
        "OutputPath": "$.Payload",
        "Parameters": {
          "Payload": {
            "notebook_path": "notebooks/2_model/cyclops_model.ipynb",
            "scenario_path_S3.$": "$.scenario_path_S3",
            "launcher_arg.$": "$.launcher_arg",
            "metadata.$": "$.metadata"
          },
          "FunctionName": "arn:aws:lambda:ca-central-1:142023388927:function:quetzal-cyclops:$LATEST"
        },
        "Retry": [
          {
            "ErrorEquals": [
              "Lambda.ServiceException",
              "Lambda.AWSLambdaException",
              "Lambda.SdkClientException",
              "Lambda.TooManyRequestsException"
            ],
            "IntervalSeconds": 2,
            "MaxAttempts": 2,
            "BackoffRate": 2
          }
        ],
        "End": true
      }
    },
    "Comment": "Quetzal cyclops model"
  }